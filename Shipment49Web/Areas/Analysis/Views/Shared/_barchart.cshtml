

<script type="text/javascript">

    var result =  @Html.Raw(ViewBag.DataPoints);
    var dataPoints =[];
    var dataPoints1 =[];
    var dataPoints2 =[];


    var result1 = '@ViewBag.status2';

    for (var i = 0; i < result.length; i++)
    {
        dataPoints.push({ label: result[i].DeviationType, y: result[i].Deviation });

    }



    $(function () {
        var chart = new CanvasJS.Chart("chartContainer7", {
      title:{
                text: "@ViewBag.Datacount",
                fontSize: 30,
                //padding: 10,
                margin: 15,
                //backgroundColor: "#FFFFE0",
                //borderThickness: 1,
                //cornerRadius: 5,
                fontWeight: "bold"
            },
            axisX: {
                labelFontSize: 12,
                labelAngle: 180
                //labelMaxWidth: 70,
                //labelWrap: true,   // change it to false
                //interval: 1
            },
            axisY: {
                gridThickness: 1
            },
            exportEnabled: true,
            animationEnabled: true,
            subtitles: [{
                text: "Vessel Name : @ViewBag.vesselname  " + "  Ranks : @ViewBag.rank " + "   Range : @ViewBag.datefrom  To  @ViewBag.dateto" + "   Total Deviation : @ViewBag.TotalNc",
                fontSize: 14
            }],
            dataPointWidth: 50,
      data: [
      {
        type: "column",
        indexLabelPlacement: "outside",
        indexLabel: "{y}",
              indexLabelOrientation: "horizontal",  // "horizontal", "vertical"

        dataPoints: dataPoints

      }
      ]
    });


        chart.render();
        var canvas = document.getElementsByClassName("canvasjs-chart-canvas")[0];
        var context = canvas.getContext('2d');
        var imageObj = new Image();
        ////imageObj.crossOrigin = "anonymous"; //https://developer.mozilla.org/en-US/docs/Web/HTML/CORS_enabled_image
        imageObj.onload = function () {
            context.drawImage(imageObj, 1, 1, 60, 70);
        }
        imageObj.src = "https://sci.work-ship.com/nwscilogo.jpg";//'http://i.imgur.com/qWTEOfN.jpg'; // https://sci.work-ship.com/nwscilogo.jpg
        alert('loaded');

    });

   

</script>




<div id="chartContainer7" style="height: 450px; width: 100%;">

</div>

