@model VesselLayer.FleetTypeClass

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page_title margin-3-bot">
    <h2>Edit FleetType</h2>
</div>
<div class="white-bg margin-3 fleet clearfix">
    <div class="row">
        <div class="col-sm-6">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.Tid)

                <div class="form-group">
                    @Html.LabelFor(model => model.FleetType, htmlAttributes: new { })
                    @Html.EditorFor(model => model.FleetType, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.FleetType, "", new { @class = "text-danger" })
                    @Html.Hidden("initialProductT", Model.FleetType)
                </div>
                <div class="row clearfix">
                    <div class="col-xs-3 col-xs-6 form-group">
                        <input type="submit" value="Update" class="btn peach-btn btn-block" />
                    </div>
                    <div class="col-sm-4 col-xs-6 back_text margin-1-top">
                        <i class="fa fa-chevron-left"></i>&nbsp; @Html.ActionLink("Go Back", "index")
                    </div>
                </div>
            }
        </div>
    </div>
</div>
